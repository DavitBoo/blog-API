{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true /* Ensure that casing is correct in imports. */,
      "strict": true,
      "downlevelIteration": true,
      "skipLibCheck": true /* Skip type checking all .d.ts files. */
    },
    "include": ["dist/**/*", "app.ts"],
    "exclude": ["node_modules"]
  }